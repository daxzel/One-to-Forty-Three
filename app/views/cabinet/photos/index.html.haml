=content_for :head do
  = javascript_include_tag "jquery.js", "jquery_ujs"
  = javascript_include_tag "gallery.js"
  = javascript_include_tag "/fancybox/jquery.fancybox-1.3.4.js"
  = stylesheet_link_tag "/fancybox/jquery.fancybox-1.3.4.css"
  = stylesheet_link_tag "pagination"
  = stylesheet_link_tag "gallery"

= t("all_photos")
.bar  
  .left.column 
    .flickr_pagination
      =will_paginate @photos, :container => false
  .right.column
    =link_to (image_tag "plus.png"), new_cabinet_photo_path(params[:car_id])

  -#gallery
    =gallery(@photos)
  .gallery
    = render :partial => "photo", :collection => @photos


- content_for :leftbar do
  = render :partial => 'leftbar'

- content_for :rightbar do
  = render :partial => 'rightbar'

