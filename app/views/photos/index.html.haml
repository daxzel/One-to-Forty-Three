%p= t("all_photos")
%p= link_to t("upload_photos"), new_photo_path

%table
  %td
    - for item in @photos
      %tr
        = image_tag item.photo.url
        = link_to "car", car_path(item.car)

- content_for :leftbar do
  = render :partial => 'leftbar'

- content_for :rightbar do
  = render :partial => 'rightbar'

